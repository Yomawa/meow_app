<% include ../partials/header %>
    <div class="container-fluid">
        <div class="thumbnail text-center">
          <div class="row">
            <div class="col-sm-12">
              <img class="img-circle" width="200" height="200" src="<%= user1.photo %>" alt="<%= user1.username %>">
              <img class="img-circle" width="200" height="200" src="<%= user2.photo %>" alt="<%= user2.username %>">
            </div>
          </div>
        </div>
      
        <div class="row">
          <div class="col-sm-6">
            <div class="thumbnail">
                <div class="caption">
                  <h4>Message me</h4>
                  <p>...</p>
                </div>
            </div>
            <div class="thumbnail">
                <div class="caption">
                  <h4>Message back</h4>
                  <p>...</p>
                </div>
            </div>
          </div>
          <div class="col-sm-3" >
            <p><% data1.businesses.forEach(function(business){ %>
                <div class="thumbnail">
                  <div class="caption">
                    <img src="<%= business.rating_img_url %>"alt="rating">
                      <h3><%= business.name %></h3>
                      <p><%= business.snippet_text %></p>
                      <p><%= business.display_phone %></p>
                      <p><%= business.location.display_address %></p>
                      <p><a href="<%= business.url %>"><%= business.url %></a></p>
                      <br><iframe
                                           width="250"
                                           height="250"
                                           frameborder="0" style="border:0"
                                           src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBPKkQTPzsPbNxor5_aJ3DQQaXd2Lc3q0g
                                             &q=<%= business.location.display_address %>" allowfullscreen>
                                     </iframe>
                </div>
              </div>                      
            <% }) %>
          </p>
        </div>
        <div class="col-sm-3">
          <p><% data2.businesses.forEach(function(business){ %>
              <div class="thumbnail" >
                <div class="caption">
                  <img src="<%= business.rating_img_url %>" alt="rating">
                    <h3><%= business.name %></h3>
                    <p><%= business.snippet_text %></p>
                    <p><%= business.display_phone %></p>
                    <p><%= business.location.display_address %></p>
                    <p><a href="<%= business.url %>"><%= business.url %></a></p>
                    <iframe
                                         width="250"
                                         height="250"
                                         frameborder="0" style="border:0"
                                         src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBPKkQTPzsPbNxor5_aJ3DQQaXd2Lc3q0g
                                           &q=<%= business.location.display_address %>" allowfullscreen>
                                   </iframe>

                </div>
              </div>
          <% }) %>
    </p>
  </div>
</div>


<% include ../partials/footer %>